<section class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1>Form Edit Warehouse</h1>
      </div>
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="#">Home</a></li>
          <li class="breadcrumb-item active">Form Edit Warehouse</li>
        </ol>
      </div>
    </div>
  </div>
  <!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">
  <div class="container-fluid">
    <div class="row">
      <!-- left column -->
      <div class="col-md-6">
        <!-- Form Add Product elements -->
        <div class="card card-primary">
          <div class="card-header">
            <h3 class="card-title">Quick Example</h3>
          </div>
          <!-- /.card-header -->
          <!-- form start -->
          <form
            role="form"
            method="POST"
            action="/warehouse-update/<%= warehouse.id %>"
          >
            <div class="card-body">
              <div class="form-group">
                <label for="exampleInputEmail1">Product Name </label>
                <input
                  type="text"
                  class="form-control"
                  id="exampleInputEmail1"
                  name="name"
                  value="<%= warehouse.name %>"
                  placeholder="Enter product name"
                />
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">Code </label>
                <input
                  type="text"
                  class="form-control"
                  id="exampleInputEmail1"
                  name="code"
                  value="<%= warehouse.code %>"
                  placeholder="Enter code"
                />
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">Size </label>
                <input
                  type="text"
                  class="form-control"
                  id="exampleInputEmail1"
                  name="size"
                  value="<%= warehouse.size %>"
                  placeholder="Enter size"
                />
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">Color </label>
                <input
                  type="text"
                  class="form-control"
                  id="exampleInputEmail1"
                  name="color"
                  value="<%= warehouse.color %>"
                  placeholder="Enter color"
                />
              </div>
              <div class="form-group">
                <label for="exampleInputEmail1">Initial Cost </label>
                <input
                  type="number"
                  class="form-control"
                  id="exampleInputEmail1"
                  name="price"
                  value="<%= warehouse.price %>"
                  placeholder="Enter initial cost"
                />
              </div>
              <!-- <div class="form-group">
                <label for="exampleFormControlFile1">Thumnail</label>
                <input
                  type="file"
                  class="form-control-file"
                  name="thumbnail"
                  id="exampleFormControlFile1"
                />
              </div> -->
              <div class="form-group">
                <label>Thumbnail </label>
                <input
                  type="text"
                  class="form-control"
                  id="exampleInputEmail1"
                  name="thumbnail"
                  value="<%= warehouse.thumbnail %>"
                  placeholder="Enter thumbnail"
                />
              </div>
              <div class="form-group">
                <label>Quantity </label>
                <input
                  type="text"
                  class="form-control"
                  id="exampleInputEmail1"
                  name="quantity"
                  value="<%= warehouse.quantity %>"
                  placeholder="Enter quantity"
                />
              </div>
            </div>
            <!-- /.card-body -->
            <div class="card-footer">
              <button type="submit" class="btn btn-primary">Submit</button>
            </div>
          </form>
        </div>
        <!-- /.card -->
      </div>
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</section>
